{
  "name": "is-map-x",
  "version": "1.0.1",
  "description": "isMap module. Detect whether or not an object is an ES6 Map.",
  "homepage": "https://github.com/Xotic750/is-map-x",
  "author": {
    "name": "Graham Fairweather",
    "email": "xotic750@gmail.com"

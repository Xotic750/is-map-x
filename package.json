    "browser"
  ],
  "main": "index.js",
  "engines": {
    "node": "*"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Xotic750/is-map-x.git"
  },
  "bugs": {
    "url": "https://github.com/Xotic750/is-map-x/issues"
  },
  "dependencies": {
    "is-object-like-x": "^1.0.1",
    "es-abstract": "^1.4.3"
  },
  "devDependencies": {
    "es5-shim": "^4.4.1",
    "es6-shim": "^0.34.0",
    "json3": "^3.3.2",
    "jscs": "^2.7.0",
    "uglify-js": "^2.6.1",
    "browserify": "12.0.1",
    "jasmine-node": "^1.14.5",
    "jsdoc-to-markdown": "^1.3.1"
  },
  "scripts": {
    "test": "jasmine-node --matchall tests/spec/",
    "style": "jscs index.js",
    "docs": "jsdoc2md --name-format code --example-lang js table index.js > README.md",
    "build": "browserify -e index.js -o lib/is-map-x.js -u 'crypto' -s returnExports",
    "build-uglify": "uglifyjs lib/is-map-x.js -o lib/is-map-x.min.js --compress unused=false,dead_code=false --keep-fnames --mangle --beautify ascii_only=true,beautify=false --source-map lib/is-map-x.map"
